  },
  "keywords": [
    "jQuery", "scroll", "fixed", "sidebar", "affix", "higher", "viewport", "large sidebar", "vertical scroll"
  ],
  "license": "MIT",
  "authors": [
    { "name": "Agustin Lascialandare", "email": "lascialandare@gmail.com" }
  ],
  "ignore": [
    "**/.*",
    "bower_components",
    "tests",
    "test*.html"
  ]
}
